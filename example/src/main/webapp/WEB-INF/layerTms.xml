<beans
		xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean name="layerTms" class="org.geomajas.layer.tms.TmsLayer" >
		<property name="baseTmsUrl" value="http://www.milieuinfo.be/orthos/" />
	</bean>

	<bean name="layerTmsProxy" class="org.geomajas.layer.tms.TmsLayer" >
		<property name="baseTmsUrl" value="http://www.milieuinfo.be/orthos/" />
		<property name="useProxy" value="true"/>
	</bean>

</beans>
