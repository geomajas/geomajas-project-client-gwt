<module rename-to="GuwExample">

	<!-- Inherited GWT modules: -->
	<inherits name="org.geomajas.widget.utility.gwt.example.WidgetUtilityExample" />

	<!-- @extract-skip-start -->

	<!-- add required languages
	<extend-property name="locale" values="nl"/>
	-->

	<!-- set target browser to compile for, use this to limit to the browser used for testing -->
	<!-- where value = "ie6/opera/gecko1_8/safari/gecko" , "gecko1_8" is FireFox 3 -->
	<set-property name="user.agent" value="gecko1_8" />
	<!-- @extract-skip-end -->

	<!-- Compile to a single file, this speeds up compilation at the penalty of creating a single large file -->
	<collapse-all-properties />

	<entry-point class="org.geomajas.widget.utility.gwt.example.client.WidgetUtilityExampleStandalone"/>
	<source path='client'/>
</module>